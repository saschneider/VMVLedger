- specific_title [t('shared.titles.audit_logs')]
- add_breadcrumb t('shared.titles.audit_logs'), :audit_logs_path
- content_for :menu do
  = render 'shared/menu'

.container
  %h1= t('shared.titles.audit_logs')

  - if @audit_logs.count > 0
    %p.lead= t('.instructions')
  - else
    %p.lead= t('.no_audit_logs')

  #list
    = render partial: 'list', locals: { audit_logs: @audit_logs }

